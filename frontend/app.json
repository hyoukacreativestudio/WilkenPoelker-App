{
  "expo": {
    "name": "WilkenPoelker",
    "slug": "wilkenpoelker",
    "owner": "hyoukaeu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2E7D32"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "de.wilkenpoelker.app",
      "buildNumber": "1",
      "associatedDomains": [
        "applinks:wilkenpoelker.de",
        "applinks:api.wilkenpoelker.de",
        "webcredentials:wilkenpoelker.de"
      ],
      "infoPlist": {
        "NSCameraUsageDescription": "WilkenPoelker benötigt Zugriff auf die Kamera für Profilbilder und Service-Anfragen.",
        "NSPhotoLibraryUsageDescription": "WilkenPoelker benötigt Zugriff auf Ihre Fotos für Profilbilder und Service-Anfragen.",
        "NSFaceIDUsageDescription": "WilkenPoelker nutzt Face ID für die sichere Anmeldung."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2E7D32"
      },
      "package": "de.wilkenpoelker.app",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ],
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "wilkenpoelker.de",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-community/datetimepicker",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#2E7D32"
        }
      ],
      "@sentry/react-native"
    ],
    "extra": {
      "eas": {
        "projectId": "eb70f600-3921-4ce9-b547-d3a83665d4d8"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/eb70f600-3921-4ce9-b547-d3a83665d4d8"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
